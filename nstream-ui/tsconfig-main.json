{
  "compilerOptions": {
    "alwaysStrict": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "incremental": true,
    "module": "es6",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "preserveConstEnums": true,
    "pretty": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES5",
    "types": ["node"]
  }
}
